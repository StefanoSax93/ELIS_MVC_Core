								<li class="nav-item">
									<a class="nav-link text-dark" asp-area="" asp-controller="Gestione" asp-action="Index">Gestione Corsi</a>
								</li>
							</ul>
						</li>

						<li class="nav-item dropdown active">
							<a class="nav-link dropdown-toggle"
								data-bs-toggle="dropdown"
								href="#"
								role="button"
								aria-expanded="false">Giorno3
							</a>
							<ul class="dropdown-menu">
								<li class="nav-item">
									<a class="nav-link text-dark" asp-area="" asp-controller="Gestione" asp-action="Partecipanti">Gestione Partecipanti</a>
								</li>
								<li class="nav-item">
